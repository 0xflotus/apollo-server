{
  "name": "apollo-engine-reporting-protobuf",
  "version": "0.0.0-beta.0",
  "description": "Core engine for Apollo GraphQL server",
  "main": "dist/index.js",
  "scripts": {
    "compile": "npm run pbjs && npm run pbts",
    "pbjs": "bash -c 'pbjs --target static-module --out dist/index.js --wrap commonjs <(grep -v \"package mdg.engine.proto\" reports.proto)'",
    "pbts": "pbts -o dist/index.d.ts dist/index.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apollographql/apollo-server/tree/master/packages/apollo-server-core"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Server",
    "Javascript"
  ],
  "author": "Apollo <community@apollographql.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollographql/apollo-server/issues"
  },
  "homepage": "https://github.com/apollographql/apollo-server#readme",
  "devDependencies": {
    "protobufjs": "~6.8.6"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
  }
}
